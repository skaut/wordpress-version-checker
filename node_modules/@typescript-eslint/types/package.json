{
  "name": "@typescript-eslint/types",
  "version": "5.2.0",
  "description": "Types for the TypeScript-ESTree AST spec",
  "keywords": [
    "eslint",
    "typescript",
    "estree"
  ],
  "engines": {
    "node": "^12.22.0 || ^14.17.0 || >=16.0.0"
  },
  "files": [
    "dist",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/typescript-eslint/typescript-eslint.git",
    "directory": "packages/types"
  },
  "bugs": {
    "url": "https://github.com/typescript-eslint/typescript-eslint/issues"
  },
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prebuild": "yarn ts-node --transpile-only ./tools/copy-ast-spec.ts",
    "build": "tsc -b tsconfig.build.json",
    "postbuild": "downlevel-dts dist _ts3.4/dist",
    "clean": "tsc -b tsconfig.build.json --clean",
    "postclean": "rimraf dist && rimraf _ts3.4 && rimraf coverage",
    "format": "prettier --write \"./**/*.{ts,js,json,md}\" --ignore-path ../../.prettierignore",
    "generate:lib": "../../node_modules/.bin/ts-node --files --transpile-only ../scope-manager/tools/generate-lib.ts",
    "lint": "eslint . --ext .js,.ts --ignore-path='../../.eslintignore'",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "funding": {
    "type": "opencollective",
    "url": "https://opencollective.com/typescript-eslint"
  },
  "typesVersions": {
    "<3.8": {
      "*": [
        "_ts3.4/*"
      ]
    }
  },
  "devDependencies": {
    "typescript": "*"
  },
  "gitHead": "9c8f9a89c098e0ebb7bebf88a4a0f07165275fa6"

,"_resolved": "https://registry.npmjs.org/@typescript-eslint/types/-/types-5.2.0.tgz"
